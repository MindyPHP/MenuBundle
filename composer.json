{
    "name": "mindy/menu-bundle",
    "license": "MIT",
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Mindy\\Bundle\\MenuBundle\\": ""
        },
        "exclude-from-classmap": [
            "./Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mindy\\Bundle\\MenuBundle\\Tests\\": "./Tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "mindy/orm-bundle": "~4.0",
        "mindy/form-bundle": "~4.0"
    },
    "authors": [
        {
            "name": "Maxim Falaleev",
            "email": "max@studio107.ru",
            "homepage": "http://studio107.ru"
        }
    ],
    "require-dev": {
        "mindy/mindy-bundle": "~4.0",
        "phpunit/phpunit": "^6.5",
        "symfony/framework-bundle": "~3.0|~4.0"
    }
}
